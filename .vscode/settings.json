{
  "window.zoomLevel": 0,
  "editor.tabSize": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  // If true, will u`se single instead of double quotes
  "prettier.singleQuote": true, // If true, puts the `>` of a multi-line jsx element at the end of the last line instead of being alone on the next line
  "prettier.jsxBracketSameLine": true,
  "prettier.printWidth": 120,
  "prettier.bracketSpacing": true,
  "[css]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  }
}
